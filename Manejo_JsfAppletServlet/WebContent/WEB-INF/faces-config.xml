<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
 <managed-bean>
  <managed-bean-name>appletServletMB</managed-bean-name>
  <managed-bean-class>org.java.manageBean.AppletServletMB</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/*</from-view-id>
  <navigation-case>
   <from-outcome>Menu</from-outcome>
   <to-view-id>/jsp/Menu.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>DemoLink_01</from-outcome>
   <to-view-id>/jsp/Ejemplo_01/Demo_JavaScripApplet.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>DemoLink_02</from-outcome>
   <to-view-id>/jsp/Ejemplo_02/Demo_AppletApplet_01.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>DemoLink_03A</from-outcome>
   <to-view-id>/jsp/Ejemplo_03/Demo_AppletServlet_JSP.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>DemoLink_03B</from-outcome>
   <to-view-id>/jsp/Ejemplo_03/Demo_AppletServlet_JSF.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>DemoLink_04</from-outcome>
   <to-view-id>/jsp/Ejemplo_04/Demo_AppletServlet.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ObjetoRequestJSP</from-outcome>
   <to-view-id>/jsp/ObjetosRequest/ObjetosRequest_JSP.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ObjetoRequestJSF</from-outcome>
   <to-view-id>/jsp/ObjetosRequest/ObjetosRequest_JSF.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
</faces-config>
