package views.categorias.comercio.renderers
{
	import mx.controls.Label;
	import mx.core.IFactory;

	public class SaldoRenderer extends Label implements IFactory
	{
		public function SaldoRenderer()
		{
			super();
		}
		
		public function newInstance():*{
			return new SaldoRenderer();
		}
		
		override protected function updateDisplayList(unscaledWidth:Number, unscaledHeight:Number):void{
			super.updateDisplayList(unscaledWidth,unscaledHeight);			
			this.setStyle("color","#DD0015");
			this.setStyle("fontWeight","bold");
			this.setStyle("fontSize","14");
		} 
	}
}