	<queryString><![CDATA[SELECT  D.c_calle_nombre || ' ' || D.c_calle_numero || ' ' || D.c_orientacion 
  || DECODE(D.c_manzana, NULL, '', ' Mz. ' || D.c_manzana) 
  || DECODE(D.c_es_monoblock, NULL, '', ' Mb. ' || D.c_es_monoblock)
  || DECODE(D.c_piso, NULL, '', ' Piso ' || D.c_piso)
  || DECODE(D.c_depto, NULL, '', ' Dpto. ' || D.c_depto)
  || ' Bº ' || B.c_descripcion  AS DOMICILIO_EMPRESA,
  l.c_nombre             AS LOCALIDAD_EMPRESA   ,
  P.c_nombre             AS PROVINCIA_EMPRESA   ,
  L.c_codigo_postal      AS CODIGOPOSTAL_EMPRESA,
  E.c_razon_social        AS NOMBRE_EMPRESA
   FROM t_vis_eva_actividades A     
  INNER JOIN t_vis_gen_suc_empresas S ON A.C_ID_SUCURSAL = S.C_ID_SUCURSAL
  INNER JOIN t_vis_gen_empresas E ON S.C_ID_EMPRESA = E.C_ID_EMPRESA
  INNER JOIN t_vis_gen_domicilios D ON S.c_id_domicilio = D.c_id_domicilio
  INNER JOIN t_vis_gen_localidades L ON D.c_id_localidad = L.c_id_localidad
  INNER JOIN t_vis_gen_barrios B ON D.c_id_barrio = B.c_id_barrio
  INNER JOIN t_vis_gen_provincias P ON L.c_id_provincia = P.c_id_provincia
  WHERE A.C_ID_ACTIVIDAD = $P{ID_ACTIVIDAD}]]></queryString>

