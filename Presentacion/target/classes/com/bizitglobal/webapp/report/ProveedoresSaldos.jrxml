	<parameter name="fecha_hasta_c" isForPrompting="true" class="java.util.Date"/>
	<parameter name="fecha" isForPrompting="true" class="java.util.Date"/>
	<parameter name="fecha_hasta_e" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT 	p.c_id_proveedor , p.c_cuit, p.c_razon_social , 
        (sum(DECODE(tc.c_signo, 1, c.c_importe_total, 0)) - sum(DECODE(tc.c_signo, -1, c.c_importe_total, 0))) as SALDO

FROM 	t_vis_prov_proveedores p,
        t_vis_gen_tipos_comprobantes tc,
        t_vis_prov_comprobantes c

WHERE 	p.c_id_proveedor = c.c_id_proveedor
AND 	c.c_id_tipo_cte = tc.c_id_tipo_cte
AND 	c.c_fecha_contable <= $P{fecha_hasta_c}
AND 	c.c_fecha_emision <= $P{fecha_hasta_e}
GROUP BY p.c_id_proveedor , p.c_razon_social , p.c_cuit
HAVING  (sum(DECODE(tc.c_signo, 1, c.c_importe_total, 0)) - sum(DECODE(tc.c_signo, -1, c.c_importe_total, 0))) <>0
ORDER BY  p.c_id_proveedor ASC,p.c_razon_social ASC]]></queryString>

