	<queryString><![CDATA[SELECT pp.c_id_proveedor as codigo,
       pp.c_cuit as CUIT,
       pp.c_razon_social as razon_social,
       ' Calle:' || gd.c_calle_nombre ||
       ' Nº:' || gd.c_calle_numero ||
       ' Piso:' || gd.c_piso ||
       ' Dpto:' || gd.c_depto ||
       ' CP:' || gd.c_codigo_postal ||
       ' Bº:'|| b.c_descripcion as domicilio,
       nvl(gt.c_nro_tlefono, '') AS c_nro_tlefono,
       pg.c_descripcion as grupo

FROM t_vis_prov_proveedores pp 
        LEFT OUTER JOIN t_vis_prov_domicilios d 
            ON (d.c_id_proveedor = pp.c_id_proveedor)
        LEFT OUTER JOIN t_vis_gen_domicilios gd 
            ON (d.c_id_domicilio = gd.c_id_domicilio)
        LEFT OUTER JOIN t_vis_gen_barrios b 
            ON (gd.c_id_barrio = b.c_id_barrio)
        INNER JOIN t_vis_prov_grupos pg 
            ON (pp.c_id_grupo = pg.c_id_grupo)
        LEFT OUTER JOIN t_vis_prov_telefonos t 
            ON(pp.c_id_proveedor = t.c_id_proveedor)
        LEFT OUTER JOIN t_vis_gen_telefonos gt 
            ON (t.c_id_telefono = gt.c_id_telefono)

WHERE  (gd.c_id_tipo_domicilio = 1 or gd.c_id_tipo_domicilio IS NULL)

ORDER BY 1 ASC]]></queryString>

