	<queryString><![CDATA[SELECT M.C_ID_MOVIMIENTO ,  O.c_nombre AS NOMBRE, O.c_apellido as APELLIDO, CO.C_DESCRIPCION AS coceptoDescripcion,
      IA.c_signo, ia.c_importe, ia.c_nro_reng, m.c_fecha AS fechaMovimiento,
      PC.titulo, PC.c_signo, PC.c_id_plan_cuenta, BA.c_descripcion AS banco,
      IA.c_id_asiento_item AS AsientoItem , DECODE(IA.c_signo,1,'debe',-1,'haber') as leyenda
FROM t_vis_fon_asientos_item IA
INNER JOIN T_VIS_FON_ASIENTOS ASI
ON ASI.c_id_asiento = ia.c_id_asiento
INNER JOIN t_vis_fon_movimientos_mp MP
ON mp.c_id_asiento_item = ia.c_id_asiento_item
INNER JOIN T_VIS_FON_MOVIMIENTOS M 
ON M.C_ID_MOVIMIENTO = mp.c_id_movimiento
INNER JOIN t_vis_cont_plan_cuenta PC
ON PC.c_id_plan_cuenta = IA.c_id_plan_cuenta
INNER JOIN T_VIS_TRA_CONCEPTOS CO
ON M.C_ID_CONCEPTO = CO.C_ID_CONCEPTO
INNER JOIN T_VIS_SEG_OPERADORES O
ON M.C_ID_OPERADOR = O.C_CODIGO
LEFT OUTER JOIN T_VIS_GEN_BANCOS BA
ON PC.c_cod_bco = ba.c_codigo
WHERE IA.c_id_asiento IN 

(SELECT ASI.c_id_asiento
FROM T_VIS_FON_MOVIMIENTOS M 
INNER JOIN t_vis_fon_movimientos_mp MP
ON M.C_ID_MOVIMIENTO = mp.c_id_movimiento
INNER JOIN t_vis_fon_asientos_item IA
ON mp.c_id_asiento_item = ia.c_id_asiento_item
INNER JOIN T_VIS_FON_ASIENTOS ASI
ON ASI.c_id_asiento = ia.c_id_asiento

WHERE  M.c_id_movimiento = $P{idMovimiento})
AND ia.c_nro_reng <> 1]]></queryString>

