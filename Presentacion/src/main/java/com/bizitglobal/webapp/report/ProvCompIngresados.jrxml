	<parameter name="id_prov_hasta" isForPrompting="true" class="java.lang.Long"/>
	<parameter name="URLImagen" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT  'Razon Social: ' || pro.c_razon_social || ' CUIT: ' || pro.c_cuit || '   Id. Proveedor: ' || pro.c_id_proveedor as datosprov,        
        to_char (cc.c_fecha_emision,  'DD/MM/YYYY') as emision, 
        to_char (fv.c_fecha_vencimiento,  'DD/MM/YYYY') as  vto,
	tc.c_descripcion_corta as tipo_cte,
        LPAD(cc.c_nro_corto, 4, '0') || '-' || LPAD(cc.c_nro_largo, 8, '0') as numero_cte,
        cc.c_importe_total as total,
        op.c_nombre || ' ' || op.c_apellido as apenom, 
        to_char(cc.c_timestamp,  'DD/MM/YYYY')as Fecha

FROM 	t_vis_prov_proveedores          pro, 
        t_vis_prov_comprobantes         cc,
        t_vis_prov_cuotas_comprobantes  fv,
        t_vis_gen_tipos_comprobantes    tc,
        t_vis_seg_operadores            op

WHERE 	pro.c_id_proveedor  = cc.c_id_proveedor
AND 	pro.c_id_operador   = op.c_codigo
AND   	cc.c_id_tipo_cte    = tc.c_id_tipo_cte
AND     cc.c_id_comprobante = fv.c_id_comprobante
AND   	tc.c_descripcion_corta in ('FA','FB','FC','FM','FT','NDA','NDB','NDC','NDM','NDT')
AND   	cc.c_fecha_emision BETWEEN $P{fecha_desde} AND $P{fecha_hasta}
AND   	pro.c_id_proveedor BETWEEN $P{id_prov_desde} AND $P{id_prov_hasta}
order by 1,4,2]]></queryString>

