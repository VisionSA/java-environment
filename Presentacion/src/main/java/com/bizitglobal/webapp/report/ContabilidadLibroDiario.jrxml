	<queryString><![CDATA[SELECT ad.c_numero_imputa as nroImputa, ad.c_asiento as nroAsiento, ad.c_fecha_contab as fechaContable, pc.titulo as titulo, 0 as debe,
       ad.c_importe as haber, ad.c_leyenda as leyenda
FROM t_cont_asientos_d ad, t_vis_cont_plan_cuenta pc
WHERE ad.c_numero_imputa = pc.c_id_plan_cuenta
AND ad.c_signo = 'D'
AND ad.c_empresa = 1 
AND ad.c_ejercicio = $P{ejercicio}
AND ad.c_fecha_contab between $P{fecha_desde} and $P{fecha_hasta}

UNION 

SELECT ad.c_numero_imputa as nroImputa, ad.c_asiento as nroAsiento, ad.c_fecha_contab as fechaContable, pc.titulo as titulo, ad.c_importe as debe,
