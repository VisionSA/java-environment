                                AND b.c_fecha_contab < $P{fecha_dos}
                                AND b.c_numero_imputa = ad.c_id_plan_cuenta)  as haber, 			
                    nvl(ad.titulo,'') as titulo
FROM dual, t_vis_cont_plan_cuenta ad 
