	<parameter name="id_Comprobante" isForPrompting="true" class="java.lang.Long"/>
	<queryString><![CDATA[SELECT DISTINCT tcomp.c_descripcion_larga AS tipo_comp, substr('0000'|| comp.c_nro_corto, -4) AS nro_corto, substr('00000000'|| comp.c_nro_largo, -8) AS nro_largo, 
                to_char(ccomp.c_fecha_vencimiento, 'DD/MM/YYYY') AS fecha_venc, ccomp.c_importe AS total_cuota , to_char(comp.c_fecha_emision, 'DD/MM/YYYY') AS fecha_emision,
                cimput.c_importe_cancela AS total_imput, to_char(ccomp.c_fecha_vencimiento, 'YYYY') AS anio, to_char(ccomp.c_fecha_vencimiento, 'MM') AS mes, 
                to_char(ccomp.c_fecha_vencimiento, 'DD') AS dia
FROM t_vis_prov_comprobantes comp
     INNER JOIN t_vis_gen_tipos_comprobantes tcomp ON comp.c_id_tipo_cte = tcomp.c_id_tipo_cte
     INNER JOIN t_vis_prov_cuotas_comprobantes ccomp ON ccomp.c_id_comprobante = comp.c_id_comprobante
     INNER JOIN t_vis_prov_ctes_imputados cimput ON cimput.c_id_cuota_comprobante_d = ccomp.c_id_cuota_comprobante
WHERE cimput.c_id_cuota_comprobante_h = (
    SELECT c_id_cuota_comprobante
    FROM t_vis_prov_cuotas_comprobantes 
    WHERE c_id_comprobante = $P{id_Comprobante})
ORDER BY anio asc, mes asc, dia asc]]></queryString>

