	<queryString><![CDATA[select ' ' || cuenta.C_ID_PLAN_CUENTA || ' ' || TITULO AS cta_contable,
  ' ' || ban.c_id_banco || ' '|| ban.c_descripcion AS banco,
  TO_CHAR(che.c_fecha_pago, 'DD/MM/YYYY') || ' ' AS fecha_pago,
  CASE WHEN che.C_TIPO  = 'A' THEN che.C_CUENTA WHEN che.C_TIPO  = 'P' THEN che.C_NUMERO WHEN che.C_TIPO  = 'T' THEN che.C_NUMERO ELSE ' ' END as nro_cheque,
  CASE WHEN mp.C_ID_FORMA_PAGO  = 2 THEN che.C_IMPORTE WHEN mp.C_ID_FORMA_PAGO  = 3 THEN che.C_IMPORTE ELSE mp.C_MONTO END as importe
from t_vis_fon_movimientos_mp mp
inner join t_vis_fon_asientos_item item on mp.c_id_asiento_item = item.c_id_asiento_item
inner join t_vis_cont_plan_cuenta cuenta ON cuenta.c_id_plan_cuenta = item.c_id_plan_cuenta
LEFT OUTER join t_vis_fon_cheques_historial his on his.c_id_asiento_item = item.c_id_asiento_item
LEFT OUTER join t_vis_fon_cheques che on his.c_id_cheque = che.c_id_cheque
LEFT OUTER join t_vis_gen_bancos ban on ban.c_id_banco = che.c_id_banco
where mp.c_id_movimiento = $P{id_movimiento}]]></queryString>

