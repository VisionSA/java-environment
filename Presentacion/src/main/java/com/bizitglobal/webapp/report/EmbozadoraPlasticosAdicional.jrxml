	<queryString><![CDATA[SELECT
  i.c_apellido       AS APELLIDO        ,
  i.c_nombres        AS NOMBRE          ,
  D.c_calle_nombre || ' ' || D.c_calle_numero || ' ' || D.c_orientacion 
  || DECODE(D.c_manzana, NULL, '', ' Mz. ' || D.c_manzana) 
  || DECODE(D.c_es_monoblock, NULL, '', ' Mb. ' || D.c_es_monoblock)
  || DECODE(D.c_piso, NULL, '', ' Piso ' || D.c_piso)
  || DECODE(D.c_depto, NULL, '', ' Dpto. ' || D.c_depto)
  || DECODE(D.c_generico,NULL, '','('|| D.c_generico||')') AS DOMICILIO,
  b.c_descripcion  AS BARRIO,
  l.c_nombre   AS LOCALIDAD       ,
  l.c_codigo_postal  AS CODIGOPOSTAL    ,
  par.c_descripcion As PARTIDO ,
  PRO.c_nombre       AS PROVINCIA       ,
  C.c_saldo_linea    AS SALDO           ,
  C.C_Limite_Credito As Limite_Credito,
  case when (C.C_LIMITE_CREDITO < 0) then 0 else C.C_LIMITE_CREDITO end as LIMITE_CREDITO_A_MOSTRAR  , 
  I.C_ID_ACTIVIDAD   AS ID_ACTIVIDAD	,
  OPER.C_CABECERA    as CABECERA      	,
  OPER.C_TEXTO       as TEXTO,
oper.c_id_operacion as Operacion
  from t_vis_tra_clientes C
  INNER JOIN t_vis_eva_individuos I ON C.c_id_individuo = I.c_id_individuo
  left outer join t_vis_gen_domicilios D ON I.c_id_domicilio = D.c_id_domicilio
  left outer join t_vis_gen_localidades L ON D.c_id_localidad = L.c_id_localidad
  left outer join t_vis_gen_partidos par ON L.c_id_partido = par.c_id_partido
  left outer join t_vis_gen_barrios B ON D.c_id_barrio = B.c_id_barrio
  left outer join t_vis_gen_provincias PRO ON L.c_id_provincia = pro.c_id_provincia
  left outer join T_vis_tra_Plastico_Operacion OPER on OPER.C_ID_OPERACION = $P{idCodOperacion}  
  WHERE c.c_id_cliente = $P{id_titular}]]></queryString>

