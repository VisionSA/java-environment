	<parameter name="Fecha_desde" isForPrompting="true" class="java.util.Date"/>
	<parameter name="Fecha_hasta" isForPrompting="true" class="java.util.Date"/>
	<parameter name="URLImagen" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select to_date(to_char(fecha,'DD/MM/YYYY'),'DD/MM/YYYY') AS FECHA,max(c_descripcion) Concepto, c_id_liq_cliente as Comprobante,c_id_cliente Cod_Cliente, 0 as Cero,
max(nombre) Nombre, sum(total) Total, (sum(iva) * c_signo) IVA, ((sum(total) - sum(iva)) * c_signo) Importe_Neto
from
(select to_date(cta.c_fecha_facturacion,'DD/MM/YYYY') as Fecha ,
det.c_nombre as tipo, 
cta.c_id_liq_cliente,
cta.c_id_cliente,
(ind.c_apellido || ', ' || ind.c_nombres) nombre,
con.c_descripcion,
det.c_id_tipo_concepto_det CondIva,
con.c_id_concepto,
cta.c_signo,
cta.c_id_tranascciones,
sum(cta.c_importe) as Total,
(case when det.c_id_tipo_concepto_det = 3 then sum(cta.c_importe) else 0 end) as Iva
FROM t_vis_tra_conceptos con
inner join t_vis_tra_conceptos_detalle det on con.c_id_concepto = det.c_id_concepto
inner join t_vis_tra_ctacte_clientes cta on det.c_id_concepto_detalle = cta.c_id_concepto_detalle
INNER join t_vis_tra_clientes cli on cli.c_id_cliente = cta.c_id_cliente
inner join t_vis_eva_individuos ind on cli.c_id_individuo = ind.c_id_individuo
where trunc(cta.c_fecha_facturacion)  between $P{Fecha_desde} and $P{Fecha_hasta}/*to_date('25/12/2009','DD/MM/YYYY') and to_date('25/01/2010','DD/MM/YYYY')*/
and cta.c_id_liq_cliente is not null and det.c_id_tipo_concepto_det in (2,3,8,9,10,11)
GROUP by cta.c_id_ctacte_cliente,to_date(cta.c_fecha_facturacion,'DD/MM/YYYY'),
det.c_nombre,
det.c_id_tipo_concepto_det,
cta.c_id_liq_cliente,
cta.c_id_cliente,
cta.c_signo,
(ind.c_apellido || ', ' || ind.c_nombres),
con.c_descripcion,con.c_id_concepto,cta.c_id_tranascciones
Order by con.c_descripcion ,cta.c_id_liq_cliente)
GROUP by c_id_cliente, fecha,c_id_liq_cliente, c_id_concepto,c_signo,c_id_tranascciones
Having sum(iva) > 0
Order by fecha,nombre, Concepto]]></queryString>

