	<queryString><![CDATA[select mov.c_fecha FECHA,to_CHAR(ctacte.c_timestamp,'HH24:MI:SS' ) AS HORA, movmp.c_monto,
movmp.c_id_forma_pago,fpago.c_descripcion,mov.c_importe,ctacte.c_importe,mov.c_id_caja,oper.c_apellido,cli.v_nombre,fon_caja.c_descripcion cajero,
clob_to_char_func(c_ticket,1000,1) AS ticket
from t_vis_fon_movimientos mov,t_vis_fon_movimientos_mp movmp,t_vis_fon_cajas fon_caja,
t_vis_seg_operadores oper,t_vis_tra_ctacte_clientes ctacte,t_vis_tra_clientes cli,t_vis_gen_formas_pagos fpago
where mov.c_id_operador=oper.c_codigo
and mov.c_id_Caja=fon_Caja.c_id_Caja
and movmp.c_id_forma_pago=fpago.c_id_forma_pago
and mov.c_id_movimiento=movmp.c_id_movimiento
and mov.c_id_movimiento=ctacte.c_id_movimiento
and ctacte.c_id_cliente=cli.c_id_cliente
and trunc(mov.c_fecha) BETWEEN $P{fecha_desde} and $P{fecha_hasta}]]></queryString>

