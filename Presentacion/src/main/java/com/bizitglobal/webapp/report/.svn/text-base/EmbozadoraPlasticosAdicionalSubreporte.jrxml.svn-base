	<parameter name="idCodOperacion" isForPrompting="true" class="java.lang.String"/>
	<parameter name="idLote" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT P.c_numero AS TARJETA, i.c_apellido AS APELLIDO, i.c_nombres AS NOMBRE
from T_VIS_TRA_PLASTICOSCLIENTES P
inner join T_VIS_TRA_CLIENTES C on (P.C_ID_CLIENTE = C.C_ID_CLIENTE)
inner join T_VIS_EVA_INDIVIDUOS I on (C.C_ID_INDIVIDUO = I.C_ID_INDIVIDUO)
where  P.C_ID_LUGARPLASTICO in (9,15)
and P.C_ID_OPERACION = $P{idCodOperacion}
and P.C_id_lote = $P{idLote}
and (C.C_ID_TITULAR = $P{idTitular} or C.C_ID_CLIENTE = $P{idTitular})
order by C.C_ADICIONAL asc]]></queryString>

