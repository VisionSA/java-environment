package com.tarjetafiel.caja.model
{
	import com.tarjetafiel.caja.vo.CajaMP;
	
	import flash.events.EventDispatcher;
	
	import mx.collections.ArrayCollection;

	[Bindable]
	public class DescargaDeValoresModel extends EventDispatcher
	{
		public var cajaMpList:ArrayCollection = new ArrayCollection();
		 	
		public function DescargaDeValoresModel()
		{
			
		}
		
		public function addCajaMp(cajaMp:CajaMP):void{
			this.cajaMpList.addItem(cajaMp);
		}
		
	}
}