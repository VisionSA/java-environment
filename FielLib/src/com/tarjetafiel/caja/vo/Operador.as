package com.tarjetafiel.caja.vo
{
	[Bindable]
	[RemoteClass( alias="com.bizitglobal.tarjetafiel.operador.negocio.Operador")]
	public class Operador
	{
			/**
		 * Identificador de negocio para la entidad operador.
		 */
		public var codigo:Number;
		
		/**
		 * Nombre de usuario para el ingreso al sistema.
		 */	
		public var username:String;
	
		/**
		 * Nombre del operador
		 */
		public var nombre:String;
		
		/**
		 * Apellido del operador. 
		 */
		public var apellido:String;
	
		/**
		 * Clave del operador.
		 */
		public var clave:String;
		
		/**
		 * Email del operador
		 */
		public var email:String;
	
		/**
		 * Estado del operador - a difinir...
		 */
		public var estado:String;
		
		/**
		 * Fecha en la que se dio de alta el operador.
		 */
		public var fechaAlta:Date;
		
		/**
		 * Operador responsable del alta.
		 */
		public var operadorAlta:Operador;
		
		/**
		 * Operador resposable de la baja.
		 */
		public var operadorBaja:Operador;
				
		/**
		 * Rol relacionado en este operador.
		 */
		public var rol:Rol;
		
		public var permiteLinea:String;
		
		public var nombreCompleto:String;
		
		public var permiteGrabar:String;
		
	}
}