package com.bizitglobal.tarjetafiel.cobranzas.negocio;

import java.util.Date;


public  class TareaPendiente  {
	
	private String descripcion;
	private int idAccion;
    private Date fecha;
    private int cantidad;
    
    public static int PRIMER_ENVIO_CARTA = 1;
    public static int TELEDIRECTO = 3;
    public static int ASIGNACION_COBRADOR = 8;
    public static int ENVIO_AVISO_URGENTE = 11;
    public static int ENVIO_AVISO_DOCUMENTADO = 12;
    public static int ENVIO_AVISO_PREJUDICIAL = 13;
    public static int LLAMADAS_TELEFONICAS = 14;
    public static int ASIGNACION_ABOGADO = 15;
    public static int LIQUIDACION_JUDICIAL = 16;
	

    public TareaPendiente() {
    	
    }

	public String getDescripcion() {
		return descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

	public Date getFecha() {
		return fecha;
	}

	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}

	public int getCantidad() {
		return cantidad;
	}

	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}

	public int getIdAccion() {
		return idAccion;
	}

	public void setIdAccion(int idAccion) {
		this.idAccion = idAccion;
	}
	
	
	
}
