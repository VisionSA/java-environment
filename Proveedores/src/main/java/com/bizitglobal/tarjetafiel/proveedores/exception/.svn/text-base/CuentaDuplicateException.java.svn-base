package com.bizitglobal.tarjetafiel.proveedores.exception;

/**
 * Es lanzada cuando existe en la base de datos una cuenta con el mismo
 * identificador que la cuenta que se quiere grabar.
 * @author Daniel
 */
public class CuentaDuplicateException extends CuentaException {
	private static final long serialVersionUID = -5610948694723631993L;

	/**
	 * Constructor con argumentos para la excepción.
	 * @param msg, Mensaje descriptivo de la excepción.
	 * @param cause, Causa de la excepción.
	 */
	public CuentaDuplicateException(String msg, Throwable cause) {
		super(msg,cause);
	}
	
}
