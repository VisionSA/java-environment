package com.bizitglobal.tarjetafiel.fondos.exception;
/**
*	Padre de las excepciones que ocurren en la clase.
*/
public class FormaPagoValorException extends Exception {
	/**
	 * 
	 */
	private static final long serialVersionUID = 4582004151819230171L;
	/**
	* Constructor por defecto de la excepcion.
	* @param msg, Mensaje descriptivo de la excepcion.
	*/
	public FormaPagoValorException (String msg) {

		super(msg);
	}
	/**
	* Constructor con argumentos para la excepcion.
	* @param msg, Mensaje descriptivo de la excepcion.
	* @param cause, Causa raiz de la excepcion.
	*/
	public FormaPagoValorException (String msg, Throwable cause) {

		super(msg,cause);
	}
}

