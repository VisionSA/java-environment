package com.bizitglobal.tarjetafiel.operador.exeption;

/**
 *	Sera lanzada cuando un usuario no posea los permisos suficientes para ejecutar alguna funcionalida.
 */
public class PermisoServiceException extends Throwable {
	
	/**
	 * Constructor por defecto.
	 */
	public PermisoServiceException() {
		super();
	}
	
	/**
	 * Constructor con argumentos.
	 * @param e, Mensaje para mostrar.
	 */
	public PermisoServiceException(String e) {
		super(e);
	}
	
}
